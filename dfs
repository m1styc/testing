nodes = {
    "0": ["1", "2", "3"],
    "1": ["3"],
    "2": ["4"],
    "3": ["5", "6"],
    "4": ["5", "7"],
    "5": ["2"],
    "6": [],
    "7": [],
}

visited = {}
res = []

for node in nodes.keys():
  visited[node] = False

root = "0"
# visited[root] = True

def dfs(node, nodes, visited):
  if not visited[node]:
    visited[node] = True
    res.append(node)

    for x in nodes[node]:
      dfs(x, nodes, visited)

